[ arithm1.imp - operacje arytmetyczne cz. 1 

        Przykladowe dane testowe:
        1. ? 1234
           > 55
        2. ? 131
           > 55
]
DECLARE
	a, aa(0:4), b, c, d
BEGIN
   READ a;

	aa(3) := a / 1;
	WRITE aa(3); [ a ]
	aa(3) := 1 * a;
	WRITE aa(3); [ a ]
	aa(3) := 1 % a;
	WRITE aa(3); [ a < 2 ? 0 : 1 ]
   b:=1;
   c:=2;
   d:=1%c;
   WRITE d;
END

